SRCS_fd1 = \
commons.f \
read_dissoc.f 

SRCS_fd2 = \
test_read_dissoc.f 

OBJS_fd1 = \
commons.o \
read_dissoc.o 

OBJS_fd2 = \
test_read_dissoc.o 

SRC_DIR_fd1 = ../../main/

SRC_DIR_fd2 = 
OBJS_DIR = obj/Debug/main/
EXE_DIR = bin/Debug/

EXE = test_read_dissoc.exe
FC = mingw32-gfortran.exe
IDIR = 
CFLAGS = -Wall -g  -J$(OBJS_DIR) $(IDIR)
LFLAGS = 
LIBS = 

VPATH = $(SRC_DIR_fd1):$(OBJS_DIR):$(SRC_DIR_fd2):$(OBJS_DIR)
OBJS = $(addprefix $(OBJS_DIR), $(OBJS_fd1) $(OBJS_fd2))

all : $(EXE)

$(EXE) : $(OBJS_fd1) $(OBJS_fd2)
	@mkdir -p $(EXE_DIR)
	$(FC) -o $(EXE_DIR)$(EXE) $(OBJS) $(LFLAGS) $(LIBS)

$(OBJS_fd1):
	@mkdir -p $(OBJS_DIR)
	$(FC) $(CFLAGS) -c $(SRC_DIR_fd1)$(@:.o=.f) -o $(OBJS_DIR)$@

$(OBJS_fd2):
	@mkdir -p $(OBJS_DIR)
	$(FC) $(CFLAGS) -c $(SRC_DIR_fd2)$(@:.o=.f) -o $(OBJS_DIR)$@

clean :
	rm -f $(OBJS_DIR)*.*
	rm -f $(EXE_DIR)$(EXE)

