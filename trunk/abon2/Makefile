SRCS_fd1 = \
absor.f \
abon2.f \
calr3.f \
calr2.f 

OBJS_fd1 = \
absor.o \
abon2.o \
calr3.o \
calr2.o 

SRC_DIR_fd1 = 
OBJS_DIR = obj/Debug/
EXE_DIR = bin/Debug/

EXE = abon2.exe
FC = mingw32-gfortran.exe
IDIR = 
CFLAGS = -Wall -g  -J$(OBJS_DIR) $(IDIR)
LFLAGS = 
LIBS = 

VPATH = $(SRC_DIR_fd1):$(OBJS_DIR)
OBJS = $(addprefix $(OBJS_DIR), $(OBJS_fd1))

all : $(EXE)

$(EXE) : $(OBJS_fd1)
	@mkdir -p $(EXE_DIR)
	$(FC) -o $(EXE_DIR)$(EXE) $(OBJS) $(LFLAGS) $(LIBS)

$(OBJS_fd1):
	@mkdir -p $(OBJS_DIR)
	$(FC) $(CFLAGS) -c $(SRC_DIR_fd1)$(@:.o=.f) -o $(OBJS_DIR)$@

clean :
	rm -f $(OBJS_DIR)*.*
	rm -f $(EXE_DIR)$(EXE)

# Dependencies of files
absor.o: \
    absor.f
abon2.o: \
    abon2.f
calr3.o: \
    calr3.f
calr2.o: \
    calr2.f

